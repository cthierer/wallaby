{
  "name": "wallaby",
  "version": "1.1.1",
  "description": "bookmarklet to save where you left off in digital comics",
  "main": "app/index.js",
  "scripts": {
    "prepublish": "run-p build",
    "build": "run-p build:*",
    "prebuild:client": "run-p clean:client",
    "build:client": "BABEL_ENV=client run-s build:client:*",
    "build:client:static": "mkdir dist && cp -r site/* dist",
    "build:client:script": "webpack",
    "build:client:tags": "riot --type es6 src dist/scripts/tags.js",
    "build:client:concat": "cp dist/scripts/client.js dist/scripts/wallaby.js && cat dist/scripts/tags.js >> dist/scripts/wallaby.js",
    "prebuild:server": "run-p clean:server",
    "build:server": "BABEL_ENV=server babel src -d lib --copy-files",
    "prebuild:doc": "run-p clean:doc",
    "build:doc": "esdoc -c esdoc.conf.json",
    "postbuild:doc": "mkdir -p dist/docs && cp -r doc/* dist/docs",
    "clean": "run-p clean:*",
    "clean:client": "rimraf dist",
    "clean:server": "rimraf lib",
    "clean:doc": "rimraf doc",
    "clean:coverage": "rimraf coverage",
    "data:migrate": "node ./lib/migrate",
    "test": "run-s test:lint test:unit",
    "pretest:unit": "run-p clean:coverage",
    "test:unit": "nyc mocha --recursive ./test/unit",
    "posttest:unit": "nyc --lines 90 --statements 90 report --reporter=lcov --reporter=json-summary",
    "test:lint": "eslint --ext .js src",
    "watch": "nodemon --watch src --watch site -e js,tag,html,css --exec './node_modules/.bin/run-s test build restart'",
    "serve": "http-server -p 3000 ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cthierer/wallaby.git"
  },
  "author": "Chris Thierer <cthierer1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cthierer/wallaby/issues"
  },
  "homepage": "https://github.com/cthierer/wallaby#readme",
  "private": true,
  "engines": {
    "node": "~6.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-async-to-bluebird": "^1.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "nyc": "^9.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "string-replace-loader": "^1.0.5",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "config": "^1.24.0",
    "form-serialize": "^0.7.1",
    "handlebars": "^4.0.6",
    "kcors": "^2.2.0",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^3.2.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.0",
    "moment": "^2.17.1",
    "octicons": "^5.0.1",
    "popsicle": "^9.0.0",
    "redis": "^2.6.5",
    "riot": "^3.0.7",
    "semver": "^5.3.0",
    "tv4": "^1.2.7",
    "uuid": "^3.0.1"
  }
}
